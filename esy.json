{
  "name": "@opam/reason",
  "version": "3.0.2",
  "dependencies": {
    "@esy-ocaml/substs": "^0.0.1",
    "@esy-ocaml/esy-installer": "^0.0.0",
    "@opam/ocamlfind": "",
    "@opam/menhir": " >= 20170418.0.0  <= 20170712.0.0",
    "@opam/utop": " >= 1.17.0",
    "@opam/merlin-extend": " >= 0.3.0",
    "@opam/result": "=1.2.0",
    "@opam/ocaml-migrate-parsetree": "*",
    "@opam/ocamlbuild": "*",
    "@opam/jbuilder": "*"
  },
  "peerDependencies": {
    "ocaml": " >= 4.2.0  < 4.6.0"
  },
  "HOW-TO-TEST-OCAML-4.04": [
    "To test with OCaml 4.04:",
    "1. Change the OCaml devDependency to: esy-ocaml/ocaml#4.4.2+esy instead of ~4.2.3000",
    "2. Rerun esy install",
    "3. Rerun esy build"
  ],
  "devDependencies": {
    "@opam/merlin": "2.5.4",
    "ocaml": "~4.2.3000"
  },
  "esy": {
    "build": [
      [ "jbuilder", "build", "--root", "." ],
      [ "sh", "-c", "(esy-installer || true)" ]
    ],
    "buildsInSource": "_build"
  }
